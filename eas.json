{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://api.eduops.cloud/api/v1",
        "EXPO_PUBLIC_API_BASE": "http://api.eduops.cloud/",
        "EXPO_PUBLIC_CLIENT_URL": "http://eduops.cloud"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://api.eduops.cloud/api/v1",
        "EXPO_PUBLIC_API_BASE": "http://api.eduops.cloud/",
        "EXPO_PUBLIC_CLIENT_URL": "http://eduops.cloud"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "http://api.eduops.cloud/api/v1",
        "EXPO_PUBLIC_API_BASE": "http://api.eduops.cloud/",
        "EXPO_PUBLIC_CLIENT_URL": "http://eduops.cloud"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
